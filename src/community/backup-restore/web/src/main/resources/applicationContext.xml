<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  <bean class="org.geoserver.platform.ModuleStatusImpl">
    <constructor-arg index="0" value="gs-backup-restore-web,"/>
    <constructor-arg index="1" value="Backup/Restore Web"/>
  </bean>
  <!-- 
    wicket ui stuff
   -->
  <bean id="backupExecutionsMenuPage" class="org.geoserver.web.MenuPageInfo">
    <property name="id" value="backupExecutions" />
    <property name="category" ref="dataCategory"/>
    <property name="titleKey" value="BackupDataPage.menu" />
    <property name="descriptionKey" value="BackupDataPage.description" />
    <property name="componentClass" value="org.geoserver.backuprestore.web.BackupDataPage" />
    <property name="order" value="1000"/>
    <property name="icon" value="database_save.png"/>
  </bean>

</beans>
